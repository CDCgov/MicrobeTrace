{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "MicrobeTrace": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": ".eslintrc.json",
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ],
            "lintFilePatterns": [
                  "src/**/*.ts",
                  "src/**/*.html"
              ]
          }
        },
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "statsJson": true,
            "outputPath": {
              "base": "dist/MicrobeTrace",
              "browser": ""
            },
            "index": "src/index.html",
            "polyfills": [
              "src/polyfills.ts"
            ],
            "tsConfig": "src/tsconfig.app.json",
            "stylePreprocessorOptions": {
              "includePaths": [
                "node_modules"
              ]
            },
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/assets/common/data",
              "src/assets/images/Logo.png",
              "src/outbreaknorm.microbetrace",
              "src/outbreakover.microbetrace",
              "src/COVID_DummySession.microbetrace",
              "src/assets/parse-csv-matrix.js",
              "src/assets/papaparse.min.js"
            ],
            "styles": [

              {
                "input": "src/microbetrace-theme.scss"
              },
              {
                "input": "src/styles.scss"
              },
              {
                "input": "node_modules/primeicons/primeicons.css"
              },
              {
                "input": "src/assets/metronic/assets/vendors/base/vendors.bundle.css"
              },
              {
                "input": "src/assets/primeng/file-upload/css/primeng.file-upload.css"
              },
              {
                "input": "src/assets/primeng/autocomplete/css/primeng.autocomplete.css"
              },
              {
                "input": "src/assets/primeng/tree/css/primeng.tree.css"
              },
              {
                "input": "src/assets/primeng/context-menu/css/primeng.context-menu.css"
              },
              {
                "input": "node_modules/leaflet/dist/leaflet.css"
              },
              {
                "input": "node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css"
              }
            ],
            "scripts": [
              "src/assets/metronic/assets/vendors/base/vendors.bundle.js",
              "src/assets/metronic/assets/demo/default/base/scripts.bundle.js"
            ],
            "browser": "src/main.ts"
          },
          "configurations": {
            "development": {
              "optimization": false,
              "sourceMap": true,
              "namedChunks": true,
              "extractLicenses": true,
              "watch": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "35mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "35mb"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/environments/version.ts",
                  "with": "src/environments/version.prod.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "none",
              "sourceMap": true,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "35mb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "MicrobeTrace:build"
          },
          "configurations": {
            "development": {
              "buildTarget": "MicrobeTrace:build:development"
            },
            "production": {
              "buildTarget": "MicrobeTrace:build:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "MicrobeTrace:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        }
      }
    },
    "MicrobeTrace-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "MicrobeTrace:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "MicrobeTrace:serve:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "cache": {
      "enabled": false
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
