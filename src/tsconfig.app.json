{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "skipLibCheck": true,
    "ignoreDeprecations": "5.0",
    "strict": false,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "alwaysStrict": false,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "strictBindCallApply": false,
    "strictPropertyInitialization": false,
    "noEmitOnError": false
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
